    vector<int> rearrangeArray(vector<int>& nums) {
        CREATE TABLE Student (
    StudentID INT PRIMARY KEY,
    Name VARCHAR(50),
    Age INT,
    Marks INT,
    City VARCHAR(50)
);



INSERT INTO Student VALUES
(1, 'Arun',   20, 85, 'Delhi'),
(2, 'Kumar',  21, 60, 'Mumbai'),
(3, 'Riya',   19, 95, 'Chennai'),
(4, 'Neha',   22, 55, 'Delhi'),
(5, 'Aman',   20, 70, 'Bangalore'),
(6, 'Priya',  21, 88, 'Mumbai'),
(7, 'Rahul',  23, 40, 'Chennai'),
(8, 'Sneha',  20, 75, 'Delhi');



SELECT * FROM Student;



SELECT Name, Age FROM Student;



SELECT * FROM Student WHERE Age > 20;



SELECT * FROM Student WHERE Name LIKE 'A%';



SELECT * FROM Student ORDER BY Age;



SELECT DISTINCT City FROM Student;



SELECT * FROM Student LIMIT 5;



SELECT City, COUNT(*) AS TotalStudents
FROM Student
GROUP BY City;



SELECT City, COUNT(*) AS TotalStudents
FROM Student
GROUP BY City
HAVING COUNT(*) > 1;



SELECT Name, Marks + 5 AS BonusMarks FROM Student;
SELECT Name, Marks - 10 AS ReducedMarks FROM Student;
SELECT Name, Marks * 2 AS DoubleMarks FROM Student;
SELECT Name, Marks / 2 AS HalfMarks FROM Student;
SELECT Name, Marks % 2 AS Remainder FROM Student;



SELECT * FROM Student WHERE Age > 18 AND Name = 'Arun';
SELECT * FROM Student WHERE Age > 20 OR Name = 'Kumar';
SELECT * FROM Student WHERE NOT Marks > 70;



SELECT * FROM Student WHERE Marks = 60;
SELECT * FROM Student WHERE Marks != 60;
SELECT * FROM Student WHERE Marks > 60;
SELECT * FROM Student WHERE Marks < 60;
SELECT * FROM Student WHERE Marks >= 60;
SELECT * FROM Student WHERE Marks <= 60;

    }
    
};